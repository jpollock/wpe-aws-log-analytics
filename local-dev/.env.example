# OpenSearch Configuration
OPENSEARCH_ENDPOINT=localhost:9200
OPENSEARCH_PASSWORD=your_password_here

# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
AWS_REGION=us-west-2
AWS_ENDPOINT_URL=http://localhost:4566  # For LocalStack

# SNS Configuration
SNS_TOPIC_ARN=arn:aws:sns:us-west-2:000000000000:log-alerts

# Log Processing Configuration
ERROR_PATTERNS=["critical", "emergency", "fatal"]

# Local Development
NODE_TLS_REJECT_UNAUTHORIZED=0  # Only for local development with self-signed certs
